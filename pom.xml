			<version>2.2</version>
		</dependency>
		<dependency>
			<groupId>com.jolly</groupId>
			<artifactId>upms-facade</artifactId>
			<version>1.6.2-SNAPSHOT</version>
			<exclusions>
				<exclusion>
					<groupId>org.slf4j</groupId>
					<artifactId>slf4j-api</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
